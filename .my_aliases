# my_aliases
## global
alias hgr="history | grep -i "
alias l="ls -hG"
alias ll="ls -lAGh"
alias c="clear"
## git
alias glog="git log --pretty=tformat:'%h %Cblue%cr%Creset %cn %Cgreen%s%Creset'"
alias gst="git status"
## misc
alias updot="cd ~/.dotfiles && git pull && bash install.sh --force && cd -"
alias instess="sudo apt -y install tmux git vim rsync bc"
